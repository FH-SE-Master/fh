<Page x:Class="SelvesSoftware.GUI.Personenverwaltung.AG.AGEinzelAnsicht"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SelvesSoftware.GUI.Personenverwaltung.AG"
      xmlns:el ="clr-namespace:SelvesSoftware.GUI.Elemente"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="AGEinzelAnsicht">

    <Grid>
        <Grid  >
            <Border BorderBrush="Black" BorderThickness="2" Background="AliceBlue">
              


            </Border>
            <Grid Margin="5,5,10,5">
                <Grid.RowDefinitions>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>
                    <RowDefinition/>

                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <el:SectionLabel Content="Kontaktdaten"/>

                <el:InputBox x:Name="FirstName" Grid.Column="1" Grid.Row="1" Text="" Focusable="False"/>
                <el:InputBox x:Name="LastName" Grid.Column="3" Grid.Row="1" Text="" Focusable="False"/>
                <el:InputLabel Content="Vorname"  Grid.Row="1"/>
                <el:InputLabel Content="Nachname" Grid.Column="2"  Grid.Row="1" />

                <el:InputBox x:Name="Email" Grid.Column="1" Grid.Row="2" Text="" Focusable="False"/>
                <el:InputBox x:Name="PhoneNr" Grid.Column="3" Grid.Row="2" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="2" Content="E-Mail" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="2" Content="Telefonnummer" />
                <el:InputBox x:Name="MobileNr" Grid.Column="1" Grid.Row="3" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="3" Grid.Column="2" Content="Sozialversicherungsnummer" />
                <el:InputBox x:Name="SVN" Grid.Column="3" Grid.Row="3" Text="" Focusable="False"/>


                <el:InputLabel Grid.Row="3" Content="Mobilnummer" />
                <el:SectionLabel Content="Adressdaten" Grid.Row="4"/>

                <el:InputBox x:Name="Street" Grid.Column="1" Grid.Row="5" Text="" Focusable="False"/>

                <el:InputLabel Grid.Row="5" Content="Straße" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="5"  Content="Nr"/>
                <Grid Grid.Column="3" Grid.Row="5" Margin="5,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition/>
                        <ColumnDefinition />
                        <ColumnDefinition/>

                    </Grid.ColumnDefinitions>
                    <el:InputBox x:Name="HNr" Text="" Margin="0,0,4,0" Focusable="False"/>
                    <el:InputBox x:Name="Etage" Grid.Column="1" Text="" Margin="1,0,2.5,0" Focusable="False"/>
                    <el:InputBox x:Name="StairNr" Grid.Column="2" Text="" Margin="2.5,0,1,0" Focusable="False"/>
                    <el:InputBox x:Name="DoorNr" Grid.Column="3" Text="" Margin="4,0,0,0" Focusable="False"/>
                    <Grid/>
                </Grid>

                <Grid Grid.Column="3" Grid.Row="4" Margin="5,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition />
                        <ColumnDefinition/>
                        <ColumnDefinition />
                        <ColumnDefinition/>

                    </Grid.ColumnDefinitions>
                    <el:SmallInputLabel  Grid.Column="0"  Content="HausNr."/>
                    <el:SmallInputLabel   Grid.Column="1"  Content="Stock"/>
                    <el:SmallInputLabel   Grid.Column="2"  Content="Stiege"/>
                    <el:SmallInputLabel   Grid.Column="3"  Content="Tür"/>
                    <Grid/>
                </Grid>

                <el:InputBox x:Name="City" Grid.Column="1" Grid.Row="6" Text="" Focusable="False"/>
                <el:InputBox x:Name="ZIP" Grid.Column="3" Grid.Row="6" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="6" Content="Stadt" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="6" Content="Postleitzahl" />

                <el:InputBox x:Name="Country" Grid.Column="1" Grid.Row="7" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="7" Grid.Column="2" Content="Staatsangehörigkeit" />
                <el:InputBox x:Name="nationality" Grid.Column="3" Grid.Row="7" Text="" Focusable="False"/>

                <el:InputLabel Grid.Row="7" Content="Land" />
                <el:InputBox x:Name="districtcommission" Grid.Column="1" Grid.Row="8" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="8" Content="Bezirkshauptmannschaft" />



                <el:SectionLabel Content="Daten" Grid.Row="9" Grid.Column="0"/>
                <Grid Grid.Column="4" Grid.Row="9" Margin="5,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <el:InputLabel Content="Von"/>
                    <el:InputLabel Content="Bis" Grid.Column="1"/>

                </Grid>

                
                <el:InputBox x:Name="EntryDate" Grid.Row="10" Grid.Column="1"  Focusable="False"/>
                <Grid Grid.Column="3" Grid.Row="10" Margin="5,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>

                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>

                    <el:InputBox x:Name="ApprovelFrom" Grid.Column="0" Focusable="False" Margin="0,-5,2.5,-5"/>
                    <el:InputBox x:Name="ApprovelTo" Grid.Column="1" Focusable="False" Margin="2.5,-5,0,-5"/>
                </Grid>
                <el:InputLabel Grid.Row="10" Content="Eintrittsdatum" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="10" Content="Bewilligungszeitraum" />

                <el:InputBox x:Name="PayperHour" Grid.Column="1" Grid.Row="11" Text="" Focusable="False"/>
                <el:InputBox x:Name="KmPay" Grid.Column="3" Grid.Row="11" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="11" Content="Stundensatz" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="11" Content="Fahrtkostenzusatz pro KM" />

                <el:InputBox x:Name="billablePayperHour" Grid.Column="1" Grid.Row="12" Text=""/>
                <el:InputLabel Grid.Row="12" Content="ausgezahlter Stundensatz" />

                <el:InputBox x:Name="Needof" Grid.Column="1" Grid.Row="13" Text="" Focusable="False"/>
                <el:InputBox x:Name="Income" Grid.Column="3" Grid.Row="12" Text="" Focusable="False"/>
                <el:InputLabel  Grid.Row="13" Content="Festgelegter Assistenzbedarf" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="12" Content="Einkommen monatlich" />

                <el:InputBox x:Name="LimitKB" Grid.Column="1" Grid.Row="14" Text="" Focusable="False"/>
                <el:InputBox x:Name="IncomeBetrag" Grid.Column="3" Grid.Row="13" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="14" Content="Höchstgrenze für KB" />
                <el:InputLabel Grid.Column="2 "  Grid.Row="13" Content="Beitrag aus Einkommen" />

                <ComboBox x:Name="CareAllowence" Grid.Row="14" Grid.Column="3" VerticalAlignment="Center" Margin="5,0" Focusable="False" IsHitTestVisible="False"/>
                <el:InputLabel Grid.Row="14" Grid.Column="2" Content="Pflegestufe" />

                <el:InputLabel Grid.Row="15" Content="Status" />
                <Grid Grid.Column="1" Grid.Row="15" Margin=" 5,0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <RadioButton x:Name="activeRadioBtn"  Content="Aktiv" VerticalAlignment="Center" IsChecked="True" IsHitTestVisible="False" />
                    <RadioButton x:Name="inactiveRadioBtn" Grid.Column="2" Content="Inaktiv" VerticalAlignment="Center" IsHitTestVisible="False"/>

                </Grid>

                <CheckBox x:Name="einfuehrungskurs" Grid.Column="2" Grid.Row="15" Content="Einführungskurs" VerticalAlignment="Center" HorizontalAlignment="Center" IsHitTestVisible="False"/>
                <CheckBox x:Name="contract" Grid.Column="3" Grid.Row="15" Content="Kontrakt" VerticalAlignment="Center" HorizontalAlignment="Center" IsHitTestVisible="False"/>

                <el:SectionLabel Content="Persönliche Assistenten" Grid.Row="16"/>
                <el:InputLabel Grid.Row="17" Content="Derzeitige Assistenten" />
                
                <ListBox x:Name="PAList" Grid.Column="1" Grid.Row="17" Grid.RowSpan="3" Margin="5,0" Focusable="False"/>
       
                <el:SectionLabel Content="Bankverbindung" Grid.Row="16" Grid.Column="2"/>
                <el:InputLabel Grid.Row="17" Grid.Column="2" Content="IBAN" />
                <el:InputBox x:Name="IBAN" Grid.Column="3" Grid.Row="17" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="18" Grid.Column="2" Content="BIC" />
                <el:InputBox x:Name="BIC" Grid.Column="3" Grid.Row="18" Text="" Focusable="False"/>
                <el:InputLabel Grid.Row="19" Grid.Column="2" Content="Kontoinhaber" />
                <el:InputBox x:Name="kontoinhaber" Grid.Column="3" Grid.Row="19" Text="" Focusable="False"/>
                <Grid Grid.Column="3" Grid.Row="20">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>

                    </Grid.ColumnDefinitions>

                    <Button x:Name="button" Grid.Column ="0"  Margin=" 5,0" Content="Zurück" Click="button_Click_1"/>
                    <Button Content="Bearbeiten" Grid.Column ="2"  Margin=" 5,0" Click="Button_Click"/>
                </Grid>
                <Grid Grid.Row="20">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Button x:Name="btnInfo" Content="Notizen"  Margin="5,0"  Grid.Column="0"/>
                    <Button x:Name="btnContact" Content="Kontaktperson"  Margin="5,0" Grid.Column="1"/>
                </Grid>

            </Grid>
        </Grid>



    </Grid>
</Page>
